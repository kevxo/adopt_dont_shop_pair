<br>
<%= form_tag "/users/new" do %>
  <%= label_tag :name %><br>
  <%= text_field_tag :name %><br>
  <%= label_tag :street_address %><br>
  <%= text_field_tag :street_address %><br>
  <%= label_tag :city %><br>
  <%= text_field_tag :city %><br>
  <%= label_tag :state %><br>
  <%= text_field_tag :state %><br>
  <%= label_tag :zip %><br>
  <%= text_field_tag :zip %><br>
  <%= submit_tag "Create User" %>
<% end %>
